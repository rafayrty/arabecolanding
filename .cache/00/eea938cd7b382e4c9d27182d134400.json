in {\r\n\r\n    .hero {\r\n        // background:linear-gradient(0deg, rgba(0, 12, 62, 0.8)),url('/assets/Banner.jpg');\r\n        background: linear-gradient(0deg, rgba(0, 12, 62, 0.8), rgba(0, 12, 62, 0.8)), url('/assets/Banner.jpg');\r\n        width: 100%;\r\n        background-size: cover;\r\n        background-position: center center;\r\n        overflow: hidden;\r\n        padding-bottom:7.4rem;\r\n\r\n        .hero-text {\r\n            padding-top: 16%;\r\n            color: #fff;\r\n            @extend %container-width;\r\n\r\n            h1 {\r\n                font-size: 2.3rem;\r\n                color: #fff;\r\n                text-align: center;\r\n                font-weight: 800;\r\n                line-height: 1.4;\r\n            }\r\n\r\n            h2 {\r\n                text-align: center;\r\n                font-size: 1.5rem;\r\n                line-height: 1.4;\r\n                font-weight: 400;\r\n                margin-top: 2rem;\r\n                color: $lightblue;\r\n\r\n            }\r\n\r\n\r\n\r\n            p {\r\n                font-size: 1.2rem;\r\n                text-align: center;\r\n                margin-top: 3.6rem;\r\n            }\r\n\r\n            #hero-cta {\r\n                font-size: 1.4rem;\r\n                background: #fff;\r\n                color: $blue;\r\n                padding: 1rem 2.3rem;\r\n                display: inline-block;\r\n                text-decoration: none;\r\n                font-weight: 600;\r\n                border-radius: 5px;\r\n                line-height: 1;\r\n\r\n            }\r\n\r\n            .cta {\r\n                text-align: center;\r\n            }\r\n        }\r\n\r\n        .hero-event-details {\r\n            margin-top: 18%;\r\n            display: flex;\r\n            flex-direction: column;\r\n            border-top: 2px solid $lightblue;\r\n            padding-top: 2rem;\r\n            @extend %container-width;\r\n\r\n            .event-detail {\r\n                display: flex;\r\n\r\n                .event-icon {\r\n                    display: inline-block;\r\n                    border: 2px solid $yellow;\r\n                    padding: .55rem 1.4rem;\r\n                    border-radius: 30px;\r\n                    display: flex;\r\n                    align-items: center;\r\n\r\n                    svg {\r\n                        display: block;\r\n                    }\r\n                }\r\n\r\n                .event-text {\r\n                    margin-right: 2rem;\r\n                    color: #Fff;\r\n\r\n                    p {\r\n                        color: $yellow;\r\n                        font-size: 1rem;\r\n                        margin: 0;\r\n                        font-weight: 600;\r\n                    }\r\n\r\n                    span {\r\n                        font-weight: 600;\r\n                    }\r\n                }\r\n\r\n                &:not(:first-child) {\r\n                    margin-top: 2rem;\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    .countdown {\r\n        @extend %container-width;\r\n        background: linear-gradient(180deg, rgba(0, 0, 0, 0.2) 0%, rgba(0, 0, 0, 0) 100%), #5195CF;\r\n        box-shadow: 0px 8px 12px rgba(38, 59, 147, 0.1);\r\n        border-radius: 8px;\r\n        padding: 2.2rem 1rem 3rem 1rem;\r\n        position:relative;\r\n        bottom:3.6rem;\r\n        &-text{\r\n            color:#fff;\r\n            text-align: center;\r\n            h1{\r\n                margin:0;\r\n                font-size:2rem;\r\n            }\r\n            h2{\r\n                margin:0;\r\n                font-weight:400;\r\n                padding:.5rem 0;\r\n                font-size:1.5rem;\r\n            }\r\n            a{\r\n                margin-top:1.24rem;\r\n                display:inline-block;   \r\n                background:#FFF;\r\n                color:$blue;\r\n                font-size:1.4rem;\r\n                font-weight:700;\r\n                border-radius:4px;\r\n                box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);\r\n                padding:.6rem 2rem;\r\n                text-decoration: none;\r\n            }\r\n        }\r\n        &-timer{\r\n            margin-top:3.2rem;\r\n            display:flex;\r\n            justify-content: space-between;\r\n            color:#FFF;\r\n            padding-bottom:1.4rem;\r\n            border-bottom:1px solid #fff;\r\n            .time{\r\n                text-align: center;\r\n                h1{\r\n                    margin:0;\r\n                    font-size:3rem;\r\n                    line-height: 1;\r\n                }\r\n                p{\r\n                    margin:0;\r\n                }\r\n                \r\n            }\r\n        }\r\n    }\r\n    .about{\r\n        @extend %container-width;\r\n\r\n        .about-container{\r\n            text-align:center;\r\n            &__text{\r\n                h1{\r\n                    color:$blue;\r\n                    font-size:2rem;\r\n                    font-weight:700;\r\n                }\r\n                p{\r\n                    color:$gray;\r\n                    font-size:.9rem;\r\n                    opacity:.7;\r\n                }\r\n            }\r\n\r\n            &__list{\r\n                margin-top:3.6rem;\r\n                &__item{\r\n\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n\r\n\r\n}","styles\\variables.scss":"$blue:#263B93;\r\n$lightblue:#7ECFE5;\r\n$yellow:#ECC91E;\r\n$gray:#1D1D1B;"},"lineCount":null}},"error":null,"hash":"ee867013ce1cecb18362a09d76a64f8d","cacheData":{"env":{}}}